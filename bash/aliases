# Bash
alias sb='. ~/bin/dotfiles/bashrc' # source bashrc
alias ea='gvim ~/bin/dotfiles/bash/aliases &'

# Git
alias g='git'

# Maven
alias mi='mvn install'
alias mc='mvn clean'
alias me='mvn eclipse:eclipse'
alias mif='mi -f'
alias mcf='mc -f'
alias mef='me -f'
alias mcr='rm -r ~/.m2/repository/com/ilodge'
alias mit='mvn install -DskipTests=false'

# Directories
alias ..='cd ..'
alias cdd='cd -'
alias cdil='cd ~/codes/ec_wspace/official/ilodge'
alias cddot='cd ~/bin/dotfiles'

alias pgtick='padsp gtick'
# Misc
#alias eclipse='~/Software/eclipse/eclipse'
